{
  "manifest_version": 2,
 
  "name": "Catana",
  "description": "This extension stops harassment with the all powerful Catana.",
  "version": "1.0",
 
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["jquery.min.js" , "content.js"],
    "all_frames": true,
    "matches": [ "http://*/*", "https://*/*" ]
  }]
}
